"\n"
"  \\NNN   the character whose ASCII code is NNN (octal)\n"
"  \\\\     backslash\n"
"  \\a     alert (BEL)\n"
"  \\b     backspace\n"
"  \\c     suppress trailing newline\n"
"  \\f     form feed\n"
"  \\n     new line\n"
"  \\r     carriage return\n"
"  \\t     horizontal tab\n"
"  \\v     vertical tab\n"
